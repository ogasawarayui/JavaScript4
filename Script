/*Q1 変数にtrueを代入し、ifの条件がtrueの時にconsole.log();で変数の中身を表示させる処理を作ってください。*/
var boolean = true;
if (boolean) {
console.log(boolean);
};
/*Q2 変数に100を代入し、ifの条件がtrueの時にconsole.log();で変数の中身を表示させる処理を作ってください。*/
var num = 100
if (num == true) {
console.log(num);
}
/*Q3 変数に"JavaScript"を代入し、ifの条件がtrueの時にconsole.log();で変数の中身を表示させる処理を作ってください。*/
var text = "JavaScript"
if (text == "true") {
console.log(text);
}
